{
  "name": "zir-nihil",
  "displayName": "zir nihil",
  "description": "VSCode Theme (dark)",
  "publisher": "zir",
  "author": {
    "name": "Zir",
    "email": "zirdev@gmail.com",
    "url": "https://github.com/zirdev"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zirdev/zir-vscode-nihil.git"
  },
  "version": "1.1.22",
  "license": "MIT",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Themes",
    "Extension Packs"
  ],
  "keywords": [
    "Theme",
    "Color Theme",
    "Dark Theme",
    "Night Theme"
  ],
  "icon": "themes/icons/zirdev.jpg",
  "galleryBanner": {
    "color": "#111E2E",
    "theme": "dark"
  },
  "contributes": {
    "themes": [
      {
        "label": "zir nihil",
        "uiTheme": "vs-dark",
        "path": "./themes/nihil-color-theme.json"
      }
    ],
    "languages": [
      {
        "id": "pgsql",
        "extensions": [
          ".pgsql",
          ".psql",
          ".sql"
        ],
        "aliases": [
          "PostgreSQL",
          "postgresql",
          "Postgres",
          "postgres",
          "SQL Postgres"
        ],
        "configuration": "./grammars/pgsql/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pgsql",
        "scopeName": "source.pgsql",
        "path": "./grammars/pgsql/PostgreSQL.tmLanguage"
      }
    ]
  },
  "extensionPack": [
    "be5invis.vscode-custom-css"
  ],
  "devDependencies": {
    "js-yaml": "^3.13.1",
    "prettier": "^1.18.2"
  }
}
